{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "react/jsx-filename-extension": ["off"],
    "linebreak-style": ["off"],
    "react/sort-comp": ["off"],
    "no-use-before-define": ["off"],
    "camelcase": 0,

  },
  "globals": {
    "it": 0,
    "expect": 0,
    "describe": 0,
    "navigator": 0,
    "fetch": 0
  }
}
